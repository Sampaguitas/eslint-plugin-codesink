{
  "name": "eslint-plugin-codesink",
  "description": "Detect most common security vulnerabilities in NodeJS projects",
  "version": "0.0.1",
  "scripts": {
    "test": "mocha tests --recursive --reporter dot",
    "lint": "eslint . --rulesdir lib/rules",
    "lint-fix": "eslint . --fix --rulesdir lib/rules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sampaguitas/eslint-plugin-codesink"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "linter",
    "security",
    "vulnerabilities",
    "advisories",
    "audit",
    "scan",
    "scanning"
  ],
  "author": "Timothee Desurmont",
  "main": "lib/index.js",
  "dependencies": {
    "@typescript-eslint/parser": "^5.27.1",
    "aws-sdk": "^2.1162.0",
    "eslint-plugin-eslint-plugin": "^4.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-utils": "^3.0.0",
    "esprima": "^4.0.1",
    "mocha": "^10.0.0",
    "requireindex": "^1.2.0",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "eslint": "^8.17.0"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "engines": {
    "node": "12.x || 14.x || >= 16"
  },
  "peerDependencies": {
    "eslint": ">=6"
  },
  "license": "ISC"
}
